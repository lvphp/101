<?xml version="1.0"?>
<phpunit>
  <filter>
      <whitelist>
        <directory suffix=".php">../src</directory>
      </whitelist>
  </filter>
  <logging>
      <log type="coverage-html" target="reports" charset="UTF-8" highlight="false" lowUpperBound="35" highLowerBound="70"/>
  </logging>
</phpunit>
